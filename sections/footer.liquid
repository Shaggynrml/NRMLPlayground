{%- style -%}
.grid-container {
  display: grid;
  grid-template-columns: 20% auto auto;
  gap: 10px;
  background-color: white;
  padding: 2px;
}

.grid-container > div {
  background-color: #f2f2f2;
  text-align: center;
  padding: 20px 0;
  font-size: 30px;
}

.item1 {
  grid-column-start: 1;
  grid-column-end: 2;
  grid-row-start:1;
  grid-row-end:2;
}
.item2{
	grid-row-start:2;
}
.item3{
	  grid-row-start:1;
    grid-row-end:4;
    grid-column-start: 2;
}.item4{
	  grid-row-start:4;
    grid-column-start: 2;
}

.item5{
	grid-row-start:1;
    grid-row-end:5;
    grid-column-start: 3;
}
.item6{
grid-column-start:1;
grid-column-end:2;
grid-row-start: 5;
}
.item7{
grid-column-start:2;
grid-column-end:3;
grid-row-start: 5;
}
  .footer {
    margin-top: {{ section.settings.margin_top | times: 0.75 | round: 0 }}px;
  }

  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }


  @media screen and (max-width: 750px) {
    #footer-menu li{
      font-size: 1rem;
    }
  }

  .page-wrapper{
    width: 100%;
    z-index: 11111;
    top: 35px;
}

footer p, footer strong, footer b, footer {
    color: #b0b0b0;
}

.footer-top {
    background: #303030;
    background-size: cover;
    background-position: center;
    padding: 0 0 20px;
    padding-top:30px;
  
}

.footer-top, .footer-bottom {
    background-color: #ededed;
}

.footer-bottom {
    padding: 15px 0;
    width: 100%;
    justify-content:center;
    display: flex;
    align-items: center;
}

.footer-site-info {
    font-size: 92.86%;
}
#footer-navigation, #footer-navigation li a:hover, .custom-footer, .custom-footer li a:hover {
    color: #d9d9d9;
}

#footer-navigation, #footer-navigation li a, .custom-footer, .custom-footer li a {
    color: #99a9b5;
}

.footer-bottom ul {
    margin: 0;
}
.inline-inside {
    font-size: 0;
    line-height: 0;
}
.clearfix:after, .clearfix:before {
    content: "";
    display: table;
}
#footer-menu li {
    display: inline-block;
    padding: 0 21px;
    position: relative;
    line-height: 1;
}

#footer-navigation, #footer-navigation li a, .custom-footer, .custom-footer li a {
    color: #fff;
    align-content:center;

}

#footer-navigation, #footer-navigation li a, .custom-footer, .custom-footer li a {
    color: #252525;
    align-content:center;

}
#footer-menu li+li:before {
    content: '';
    width: 0;
    height: 100%;
    position: absolute;
    left: -1px;
    top: 0;
    font-size: 0;
}


navigation li a, .custom-footer, .custom-footer li a {
    color: #252525;
    padding-top: 15px;
}

.footer-socials {
    display: flex;
    list-style: none;
    margin: 0 1rem !important;
    padding: 0;
    text-align: center;
}


.footer-bottom a:hover {
    color: #d9d9d9;
}


footer p, footer li {
    font-size: 15px;
    line-height: 22px;
}


.widget {
    margin-bottom: 50px;
}

.footer-title {
    margin-bottom: 40px;
    color: #fff;
    font-weight: 500;
    text-transform: capitalize;
    padding-bottom: 15px;
    font-size: 16px;
    position: relative;
}

.footer-title:after {
    width: 50px;
    background: #fff;
    opacity: 0.2;
    height: 1px;
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
}

.gem-contacts-item {
    padding-top: 10px;
    font-size: 15px;
}

.gem-contacts-item i {
    padding-right: 10px;
}

footer .widget ul {
    list-style: none;
    margin-top: 5px;
}

.posts li {
    border-bottom: 1px solid #393d50;
    padding-bottom: 12px;
    padding-top: 6px;
}

footer p, footer li {
    font-size: 15px;
    line-height: 22px;
}

.gem-pp-posts-date {
    color: #00bcd4;
    font-size: 89.5%;
}

footer p {
    line-height: 24px;
    margin-bottom: 10px;font-size: 15px;
}


textarea {
    overflow: auto;
    resize: vertical;
}


.footer-site-info{
  padding-top: 10px;
}
.copyright-footer{
  padding-left: 10px;
  color:#252525;
}
.links-footer{
  margin: 0 1rem;
}

{%- endstyle -%}
